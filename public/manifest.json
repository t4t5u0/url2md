{
  "name": "名前",
  "manifest_version": 2,
  "version": "1.0",

  "icons": { "32": "icon_32.png" },
  "browser_action": {
    "default_icon": "icon_32.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["main.js"]
    }
  ],
  "permissions": ["activeTab", "clipboardWrite"]
}
