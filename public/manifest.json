{
  "name": "click here",
  "manifest_version": 2,
  "version": "0.1",

  "icons": { "32": "icon_32.png" },
  "browser_action": {
    "default_icon": "icon_32.png",
    "default_popup": "popup.html"
  },

  "background": {
    "scripts": ["main.js"],
    "persistant": false
  },

  "permissions": ["clipboardWrite", "activeTab"]
}
