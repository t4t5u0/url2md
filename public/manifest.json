{
  "name": "名前",
  "manifest_version": 2,
  "version": "1.0",

  "icons": { "32": "icon_32.png" },
  "browser_action": {
    "default_icon": "icon_32.png",
    "default_popup": "popup.html"
  },

  "background": {
    "scripts": ["main.js"],
    "persistent": false
  },

  "permissions": ["activeTab", "clipboardWrite"]
}
